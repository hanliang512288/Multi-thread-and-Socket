public class Worker {
	public int port;
	public String ip;
	public String state;
	public int workload;
	
	public Worker(int port, String ip){
		this.port=port;
		this.ip=ip;
		workload=0;
		state="Y";
		
	}
	
	public void setState(String s){
		state=s;
	}
	
	public void setWorkLoad(int load){
		load=workload;
	}
	
}